function confirmLogout() {	if (confirm("Are you sure you wish to logout of referee services?")) {		var linkURL = document.forms[0].LogoutRedirect.value;		document.write();		self.location.replace(linkURL);	}}function openHelp(OpenURL,wToolbar,wWidth,wHeight) {	var wString = "";	var newWnd		if (wToolbar == false) {		wString = "toolbar=true,menubar=false,location=false,";	} else {		wString = "toolbar,location,menubar,";	}			if ( !newWnd || newWnd.closed ) {		newWnd = window.open(OpenURL,'',wString +"scrollbars,resizable,WIDTH="+wWidth+",HEIGHT="+wHeight)		newWnd.focus()	} else {	 	newWnd.focus()	}return}function openURLWithISSN(URL,suffix) {	  var newWnd	  var issnval = document.forms[0].ISSN.value;	  var wString = "toolbars,scrollbars,resizable,WIDTH=700,HEIGHT=500";	if (issnval == "") {		newWnd=window.open(URL+suffix,'',wString);	} else {			newWnd=window.open(URL+document.forms[0].ISSN.value+suffix,'',wString);	}	newWnd.focus()}